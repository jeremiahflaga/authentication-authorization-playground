@page
@model SampleSite.Pages.Page1Model
@{
    ViewData["Title"] = "Page 1";
}

<h1>Page 1</h1>

<hr />

<div>
    <h2>User</h2>
    @foreach (var identity in User.Identities)
    {
        <div>Authentication Type: @identity.AuthenticationType</div>
        <table class="table">
            @foreach (var claim in identity.Claims)
            {
                <tr>
                    <td>@claim.Type</td>
                    <td>@claim.Value</td>
                </tr>
            }
        </table>
    }
</div>